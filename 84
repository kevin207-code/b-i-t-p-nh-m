#include <iostream>
using namespace std;

void nhapmang1chieu(float A[], int);


int main()
{
	int n;
	cout << "nhap so phan tu can nhap cua mang: ";
	cin >> n;
	float A[100];
	nhapmang1chieu(A, n);
}
void nhapmang1chieu(float A[], int n)
{
	
	for (int i = 0; i < n; i++)
	{
		
		cout << "nhap A[" << i << "] : ";
		cin >> A[i];
	}
}
